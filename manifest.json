{
    "manifest_version": 3,
    "name": "Lowest Price Finder",
    "version": "1.0",
    "description": "Finds lowest price in the store",
    "permissions": ["activeTab", "scripting"],
    "host_permissions": ["<all_urls>"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
    }
  }